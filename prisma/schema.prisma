generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            String      @id @default(uuid())
  createdAt     DateTime    @default(now()) @map(name: "created_at")
  updatedAt     DateTime    @updatedAt @map(name: "updated_at")
  email         String      @unique
  types         AlertTypes[]
}

enum AlertTypes {
  lt10sats
  gt10sats
  lt50sats
  gt50sats
}